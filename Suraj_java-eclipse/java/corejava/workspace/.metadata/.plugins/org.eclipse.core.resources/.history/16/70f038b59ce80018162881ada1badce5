	int[] desc(int a[]){
		int temp;
		for (int index = 0; index < arr.length; index++) {
			for (int index1 = 0; index1 < arr.length - index - 1; index1++) {
				if (arr[index1] < arr[index1 + 1]) {
					temp = arr[index1];
					arr[index1] = arr[index1 + 1];
					arr[index1 + 1] = temp;
				}
			}

		}
		return arr;
	}
